> {\n    const { name, value } = e.target;\n    setFormData(prevState => ({\n      ...prevState,\n      [name]: value\n    }))\n  }\n\n  const handleDropDownChange = (e) => {\n    const { value } = e.target;\n    setDestinationId(value);\n    setFormData(prevState => ({\n      ...prevState,\n      destination_id: value\n    }))\n  }\n\n  return (\n    <div>\n      <h3>Edit an Activity</h3>\n      <form onSubmit={(e) => {\n        e.preventDefault()\n        props.handleUpdate(id, formData)\n      }}>\n        <label htmlFor=\"name\" className='form-label'>Name:</label>\n        <input\n          type='text'\n          name=\"name\"\n          placeholder='Activity Name'\n          value={formData.name}\n          onChange={handleChange}\n        />\n        <label htmlFor=\"description\" className='form-label'>Description:</label>\n        <input\n          type='text'\n          name=\"description\"\n          placeholder='Activity Description'\n          value={formData.description}\n          onChange={handleChange}\n        />\n        <label htmlFor=\"img_url\" className='form-label'>Image:</label>\n        <input\n          type='url'\n          name=\"img_url\"\n          placeholder='Activity Image (Enter URL for Image)'\n          value={formData.img_url}\n          onChange={handleChange}\n        />\n        <label htmlFor=\"price\" className='form-label'>Price:</label>\n        <input\n          type='number'\n          name=\"price\"\n          placeholder='Activity Price'\n          value={formData.price}\n          onChange={handleChange}\n        />\n        <label htmlFor=\"activity_url\" className='form-label'>Link to Activity:</label>\n        <input\n          type='url'\n          name=\"activity_url\"\n          placeholder='Link to Activity Website'\n          value={formData.activity_url}\n          onChange={handleChange}\n        />\n         <select defaultValue='default' onChange={handleDropDownChange}>\n          <option disabled value='default'>-- Select a Country --</option>\n          {props.destinations.map(country => (\n            <option value={country.id} key={country.id}>{country.country_name}</option>\n          ))}\n        </select>\n        <button>Submit</button>\n      </form>\n    </div>\n  );\n}\n\nexport default EditActivity;",{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":48,"column":94,"nodeType":"82","endLine":48,"endColumn":136},{"ruleId":"83","severity":1,"message":"84","line":33,"column":6,"nodeType":"85","endLine":33,"endColumn":24,"suggestions":"86"},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.","ArrayExpression",["89"],"no-global-assign","no-unsafe-negation",{"desc":"90","fix":"91"},"Update the dependencies array to be: [id, props.activities]",{"range":"92","text":"93"},[885,903],"[id, props.activities]"]